<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias alias="cart" type="com.spring.collabee.biz.cart.CartVO"/>
		<typeAlias alias="orderMember" type="com.spring.collabee.biz.order.OrderMemberVO"/>
		<typeAlias alias="memberVO" type="com.spring.collabee.biz.member.MemberVO"/>
		<typeAlias alias="goods" type="com.spring.collabee.biz.goods.GoodsVO"/>
	    <typeAlias alias="categories" type="com.spring.collabee.biz.collections.CategoriesVO"/>
	    <typeAlias alias="coupons" type="com.spring.collabee.biz.coupons.CouponsVO"/>    	
	    <typeAlias alias="division" type="com.spring.collabee.biz.collections.DivisionVO"/>
		<typeAlias alias="faq" type="com.spring.collabee.biz.faq.FaqVO"/>
		<typeAlias alias="inquiry" type="com.spring.collabee.biz.inquiry.InquiryVO"/>
	    <typeAlias alias="board" type="com.spring.collabee.biz.board.BoardVO"/>
	    <typeAlias alias="goodsReview" type="com.spring.collabee.biz.goods.GoodsReviewVO"/>
   		<typeAlias alias="goodsQnA" type="com.spring.collabee.biz.goods.GoodsQnAVO"/>
  </typeAliases>
  
  
</configuration>